# Environment variables for automated builds
# Copy this file to automation.env and fill in your values

# Firebase Configuration
FIREBASE_APP_ID=your_firebase_app_id_here
FIREBASE_TOKEN=your_firebase_token_here

# App Configuration
APP_IDENTIFIER=com.deliveryapp
APP_NAME=App Driver
PACKAGE_NAME=com.deliveryapp
VERSION_NAME=1.0.0
VERSION_CODE=1
MIN_SDK_VERSION=21
TARGET_SDK_VERSION=34

# Email Configuration (for local testing)
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
NOTIFICATION_EMAIL=your_notification_email@gmail.com

# Android Signing (for release builds)
KEYSTORE_PASSWORD=your_keystore_password
KEY_ALIAS=your_key_alias
KEY_PASSWORD=your_key_password
